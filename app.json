{
  "expo": {
    "name": "EFISTRAC",
    "slug": "cuaca-benoa-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "moca",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/EFISTRAC.png",
      "resizeMode": "contain",
      "backgroundColor": "#040424"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "name":"EFISTRAC",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.zentexe.cuacabenoaapp"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "12b056ac-7fb5-4c06-9ec2-02ab453789d1"
      }
    }
  }
}
